Class Webhook
    Shared Event CheckTemp() RaiseEvent Every 15 Seconds
        Dim t As Float = TMP102.GetTemp()
        Dim tStr As String = t.ToString
        
        Dim j As Json = New Json
        j.Add("value1", t)
        Dim jStr As String = j.ToString
        
        Dim success As Boolean = My.mcCloud.Webhook(1, jStr, 30)
        
    End Event
    Shared Event Blink() RaiseEvent Every 1 Seconds
        My.LedGreen.Toggle()
    End Event
End Class 
