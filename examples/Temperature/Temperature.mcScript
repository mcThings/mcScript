Class Temperature
    Shared Event GetTemp() RaiseEvent Every 1 Minutes
        My.LedGreen.Set()
        
        Dim TempC As Float = TMP102.GetTemp()
        Dim TempF As Float = TMP102.ToFarenheit(TempC)
        
        If TempC > 25 Then
            My.LedRed.Set()
        Else
            My.LedRed.Clear()
        End If
        
        My.LedGreen.Clear()
    End Event
End Class 
